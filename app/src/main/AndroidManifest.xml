<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.ahsru.corki.phone">

<uses-permission android:name="android.permission.CALL_PHONE"></uses-permission>  <!--打电话权限-->
<uses-permission android:name="android.permission.VIBRATE"></uses-permission><!--振动权限-->
    <application
            android:allowBackup="true"
            android:icon="@mipmap/phone1"
            android:label="@string/app_name"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".PhoneActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>